{
	"name": "trz",
	"private": true,
	"author": "JAYNE <JAYNE@CHENZHENYUAN.COM>",
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"start": "npx webpack s",
		"build:all": "lerna run build --ignore=@trz/requests",
		"build:type": "lerna run build --scope=@trz/type",
		"build:serialize": "npm run build:type && lerna run build --scope=@trz/serialize",
		"build:uri": "npm run build:type && npm run build:serialize && lerna run build --scope=@trz/uri",
		"build:util": "npm run build:type && lerna run build --scope=@trz/util"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@babel/cli": "^7.16.8",
		"@babel/core": "^7.16.12",
		"@babel/eslint-parser": "^7.16.5",
		"@babel/eslint-plugin": "^7.16.5",
		"@babel/plugin-proposal-decorators": "^7.16.7",
		"@babel/plugin-proposal-object-rest-spread": "^7.16.7",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.16.10",
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-react": "^7.16.7",
		"@babel/preset-typescript": "^7.16.7",
		"@babel/runtime-corejs3": "^7.16.8",
		"@types/async": "^3.2.12",
		"@types/core-js": "^2.5.5",
		"@types/enzyme": "^3.10.11",
		"@types/enzyme-adapter-react-16": "^1.0.6",
		"@types/jest": "^27.4.0",
		"@types/lodash.merge": "^4.6.6",
		"@types/node": "^17.0.12",
		"@types/webpack": "^5.28.0",
		"@types/webpack-dev-server": "^4.7.2",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"babel-loader": "^8.2.3",
		"babel-preset-minify": "^0.5.1",
		"core-js": "^3.20.3",
		"eslint": "^8.7.0",
		"eslint-plugin": "^1.0.1",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-prettier": "^4.0.0",
		"html-webpack-plugin": "^5.5.0",
		"jest": "^27.4.7",
