{
  "name": "@trz/uri",
  "version": "0.1.3",
  "description": "一个简单的Uri工具库",
  "author": "ZHENYUAN <JAYNE@CHENZHENYUAN.COM>",
  "homepage": "https://github.com/chenzhenyuan/trz/tree/release/packages/url#readme",
  "license": "MIT",
  "main": "lib/uri.js",
  "types": "lib/uri.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chenzhenyuan/trz.git"
  },
  "scripts": {
    "clean"  : "rimraf lib",
    "build"  : "npm run clean && babel src --out-dir lib --extensions .ts,.tsx --delete-dir-on-start --no-comments --source-type=module --config-file='../../.babelrc' && tsc",
    "test"   : "npm run build && jest",
    "release": "npm run build && npm publish --access=public --registry=https://registry.npmjs.org"
  },
  "bugs": {
    "url": "https://github.com/chenzhenyuan/trz/issues"
  },
  "dependencies": {
    "@trz/util": ">=0.3.0"
  }
}
