{
  "name": "@trz/uri",
  "version": "0.2.3",
  "description": "一个简单的Uri工具库",
  "author": "ZHENYUAN <JAYNE@CHENZHENYUAN.COM>",
  "homepage": "https://github.com/chenzhenyuan/trz/tree/release/packages/url#readme",
  "license": "MIT",
  "main": "./uri.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib", "uri.js", "uri.d.ts", "README_CN.md"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chenzhenyuan/trz.git"
  },
  "scripts": {
    "clean"  : "rimraf ./uri.js ./uri.d.ts",
    "build"  : "npm run clean && babel src --out-dir . --extensions .ts,.tsx --no-comments --source-type=module --config-file='../../.babelrc' && tsc",
    "release": "npm run build && npm publish --access=public --registry=https://registry.npmjs.org"
  },
  "bugs": {
    "url": "https://github.com/chenzhenyuan/trz/issues"
  },
  "dependencies": {
    "@trz/type": ">=0.2.0",
    "@trz/util": ">=0.3.0"
  }
}
